{
  "name": "ngx-accordion-table",
  "version": "1.0.2",
  "description": "A small Angular2/Angular4 component to build accordion tables (tables which each row have another table) can be used for example to display detailed information from each row",
  "scripts": {
    "build": "ng-packagr -p ng-package.json && cp js dist/ -r && cp README.md dist/"
  },
  "keywords": [
    "accordion",
    "table",
    "table-accordion",
    "table-inside-table",
    "angular2",
    "angular4",
    "angular-accordion",
    "angular-table",
    "angular-accordion-table",
    "angular-component-accordion"
  ],
  "author": "Lucas Ventura",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lucasventurasc/ngx-accordion-table.git"
  },
  "dependencies": {
    "@angular/common": "^5.2.0",
    "@angular/core": "^5.2.0",
    "@angular/platform-browser": "^5.2.0"
  },
  "devDependencies": {
    "@angular/compiler": "^5.2.9",
    "@angular/compiler-cli": "^5.2.9",
    "ng-packagr": "^2.4.1",
    "rollup": "^0.57.1",
    "typescript": "^2.8.1",
    "uglify-js": "^3.3.18"
  },
  "private": false
}
